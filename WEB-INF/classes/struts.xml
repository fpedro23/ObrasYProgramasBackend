<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="true" />
	
	<package name="Obras y Programas" namespace="/" extends="struts-default">
         <action name="operacionProgramas" class="servletObrasYProgramas.ServletProgramas" method="execute">
             <result name="consultarProgramas">/consultaGeneralProgramas.jsp</result>
         </action>
         
         <action name="operacionProgramasConsultaEspecifica" class="servletObrasYProgramas.ServletProgramas" method="consultaProgramasEspecifica">
             <result name="consultarProgramas">/consultaGeneralProgramas.jsp</result>
         </action>
         
         <action name="generarFichaTecnica" class="servletObrasYProgramas.ServletObras" method="generarFichaTecnica">
             <result name="fichaTecnicaObras">/fichaTecnicaObra.jsp</result>
         </action>
         
         <action name="consultarEstados" class="servletObrasYProgramas.ServletObras" method="consultarEstados">
             <result name="respuestaListaJson">/Views/ListaJSON.jsp</result>
         </action>
         
         <action name="consultarDependencias" class="servletObrasYProgramas.ServletObras" method="consultarDependencias">
             <result name="respuestaListaJson">/Views/ListaJSON.jsp</result>
         </action>
         
         <action name="consultarImpactos" class="servletObrasYProgramas.ServletObras" method="consultarImpactos">
             <result name="respuestaListaJson">/Views/ListaJSON.jsp</result>
         </action>
         
         <action name="consultarInauguradores" class="servletObrasYProgramas.ServletObras" method="consultarInauguradores">
             <result name="respuestaListaJson">/Views/ListaJSON.jsp</result>
         </action>
         
         <action name="consultarPoblacionesObjetivo" class="servletObrasYProgramas.ServletObras" method="consultarPoblacionesObjetivo">
             <result name="respuestaListaJson">/Views/ListaJSON.jsp</result>
         </action>
         
         <action name="consultarClasificaciones" class="servletObrasYProgramas.ServletObras" method="consultarClasificaciones">
             <result name="respuestaListaJson">/Views/ListaJSON.jsp</result>
         </action>
         
         <action name="consultarInversiones" class="servletObrasYProgramas.ServletObras" method="consultarInversiones">
             <result name="respuestaListaJson">/Views/ListaJSON.jsp</result>
         </action>
         
         <action name="consultarTiposDeObra" class="servletObrasYProgramas.ServletObras" method="consultarTiposDeObra">
             <result name="respuestaListaJson">/Views/ListaJSON.jsp</result>
         </action>
         
	</package>
</struts>
